<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="ElectronicCalculator.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:ElectronicCalculator.Views"
	xmlns:local="clr-namespace:ElectronicCalculator">

	<TabBar>
		<ShellContent
			Title="Home"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:HomePage}" />
		<ShellContent
			Title="Voltage Regulator"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:VoltagePage}" />
		<ShellContent
			Title="CCR"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:ConstantPage}" />
		<ShellContent
			Title="Info"
			Icon="iconwebview.png"
			ContentTemplate="{DataTemplate views:InfoPage}" />
	</TabBar>
</Shell>
